    "node-fetch": "^3.3.1",
    "uuid": "^8.3.2",
    "validate.js": "^0.13.1"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.17.0",
    "@types/aws-lambda": "^8.10.17",
    "@types/node": "^10.12.18",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
