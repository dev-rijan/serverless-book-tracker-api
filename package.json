{
  "name": "book-tracker",
  "version": "1.0.0",
  "description": "book tracker app",
  "main": "handler.js",
  "scripts": {
    "deploy:prod": "sls deploy --stage prod",
    "package:prod": "sls package --stage prod",
    "undeploy:prod": "sls remove --stage prod",
    "start:offline": "sls offline start --stage dev",
    "start:offline:debug": "SLS_DEBUG=1 sls offline start --stage dev",
    "dynamodb:install": "sls dynamodb install",
    "lint": "eslint --fix",
    "prettier": "prettier -w src",
    "test": "jest"
  },
  "dependencies": {
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "aws-sdk": "^2.848.0",
    "axios": "^1.3.5",
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "node-fetch": "^3.3.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.17.0",
    "@types/aws-lambda": "^8.10.17",
    "@types/jest": "^29.5.0",
    "@types/node": "^10.12.18",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "ajv": "^8.12.0",
    "dynamodb-localhost": "https://github.com/99x/dynamodb-localhost#db30898f8c40932c7177be7b2f1a81360d12876d",
    "esbuild": "^0.14.39",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "install": "^0.13.0",
    "jest": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "serverless": "^3.15.2",
    "serverless-api-gateway-throttling": "^1.1.0",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-dynamodb-local": "^0.2.39",
    "serverless-esbuild": "^1.27.0",
    "serverless-offline": "^6.8.0",
    "serverless-plugin-aws-alerts": "^1.7.1",
    "serverless-plugin-subscription-filter": "^1.0.7",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.1.5"
  },
  "overrides": {
    "serverless-dynamodb-local": {
      "dynamodb-localhost": "https://github.com/99x/dynamodb-localhost#db30898f8c40932c7177be7b2f1a81360d12876d"
    }
  },
  "jest": {
    "roots": [
      "<rootDir>/tests/feature/comment/"
    ],
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node",
      "vue"
    ]
  },
  "author": "Rijan adhikari",
  "license": "MIT"
}